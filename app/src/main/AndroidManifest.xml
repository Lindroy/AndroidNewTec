        <activity android:name="com.lin.afinal.AfinalActivity" />
        <activity
            android:name="com.lin.recyclerview.RecyclerViewActivity"
            android:theme="@style/listDividerTheme" />
        <activity android:name="com.lin.json.NativeJsonParseActivity" />
        <activity android:name="com.lin.json.GsonActivity" />
        <activity android:name="com.lin.json.FastJsonActivity" />
        <activity android:name="com.lin.pulltorefresh.PullToRefreshActivity" />
        <activity android:name="com.lin.pulltorefresh.PullToRefreshListActivity" />
        <activity android:name="com.lin.glide.GlideActivity" />
        <activity android:name="com.lin.glide.GlideBaseActivity" />
        <activity android:name="com.lin.glide.GlideRecyclerViewActivity" />
        <activity android:name="com.lin.glide.GlideTransformActivity"></activity>
